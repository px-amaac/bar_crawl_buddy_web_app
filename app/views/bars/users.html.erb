<% provide(:title, @title) %>
<div class="row">
	<aside class="span4">
		<section>
			<h1><%= @bar.bar_name %></h1>
			<span><%= link_to "View Bar", @bar %> </span>
		</section>
	</aside>
	<div class="span8">
		<h3><%= @title %></h3>
		<% if @users.any? %>
			<ul class="users">
				<%= render @users %>
			</ul>
			<%= will_paginate %>
		<% end %>
	</div>

</div>